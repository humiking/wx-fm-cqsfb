<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.WxFmListMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.WxFmList">
		<id column="id" property="id" />
		<result column="NAME" property="name" />
		<result column="poster" property="poster" />
		<result column="artistor_name" property="artistorName" />
		<result column="total_duration" property="totalDuration" />
		<result column="status" property="status" />
		<result column="weight" property="weight" />
		<result column="publish_status" property="publishStatus" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="publish_time" property="publishTime" />
	</resultMap>
	
	<insert id="addWxFmList" parameterType="com.stylefeng.guns.common.persistence.model.WxFmList" useGeneratedKeys="true" keyProperty="wxFmList.id" keyColumn="GENERATED_KEY">
	     INSERT INTO wx_fm_list 
	     (NAME,
	      poster,
	      artistor_name,
	      total_duration,
	      status,
	      weight,
	      publish_status,
	      create_time,
	      update_time)
	     VALUES
	     (#{wxFmList.name},
	      #{wxFmList.poster},
	      #{wxFmList.artistorName},
	      #{wxFmList.totalDuration},
	      0,
	      #{wxFmList.weight},
	      0,
	      #{wxFmList.createTime},
	      #{wxFmList.updateTime})
	
	
	</insert>

</mapper>
